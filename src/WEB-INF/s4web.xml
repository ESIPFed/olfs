<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>


    <servlet>
        <servlet-name>
            s4
        </servlet-name>

        <servlet-class>
            opendap.olfs.OLFS
        </servlet-class>

        <init-param>
            <param-name>DebugOn</param-name>
            <param-value>showRequest CE</param-value>
        </init-param>

        <init-param>
            <param-name>BackEndServer</param-name>
            <param-value>dataportal.ucar.edu</param-value>
        </init-param>

        <init-param>
            <param-name>BackEndServerPort</param-name>
            <param-value>2002</param-value>
        </init-param>

        <init-param>
            <param-name>FileSystemPrefix</param-name>
            <param-value>/home/pwest/tests/data</param-value>
        </init-param>

        <init-param>
            <param-name>TargetPathSeperator</param-name>
            <param-value>/</param-value>
        </init-param>


    </servlet>


    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/s4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/s4/*</url-pattern>
    </servlet-mapping>




</web-app>




